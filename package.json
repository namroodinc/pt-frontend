{
  "name": "react-portfolio",
  "version": "1.0.0",
  "description": "Boilerplate for a React project",
  "main": "src/server/server.js",
  "scripts": {
    "clean-lib": "rimraf lib",
    "clean-public": "rimraf public",
    "clean": "npm-run-all --parallel clean-lib clean-public",
    "dev-server": "node lib/server/webpack",
    "server": "nodemon lib/server/server.js",
    "watch-js": "babel src -d lib -w",
    "start": "npm-run-all --parallel watch-js dev-server server",
    "build": "npm run clean && babel src -d lib && webpack --config webpack.config.prod.js",
    "postinstall": "npm run build",
    "production": "node lib/server/server.js",
    "lint": "eslint -c .eslintrc src",
    "test": "echo No tests at the moment",
    "precommit": "npm run lint && npm run test"
  },
  "engines": {
    "node": "~6.11.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/namroodinc/react-portfolio.git"
  },
  "keywords": [
    "React",
    "MobX"
  ],
  "author": "Ashoor Namrood",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/namroodinc/react-portfolio/issues"
  },
  "homepage": "https://github.com/namroodinc/react-portfolio#readme",
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "eslint": "^4.12.1",
    "eslint-config-react": "^1.1.7",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "npm-run-all": "^4.1.2"
  },
  "dependencies": {
    "autoprefixer": "^7.1.6",
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "classnames": "^2.2.5",
    "concurrently": "^3.5.1",
    "css-hot-loader": "^1.3.4",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.15.6",
    "extract-text-webpack-plugin": "^3.0.2",
    "lodash": "^4.17.4",
    "marked": "^0.3.7",
    "material-ui": "^1.0.0-beta.22",
    "material-ui-icons": "^1.0.0-beta.17",
    "mobx": "^3.3.2",
    "mobx-react": "^4.3.5",
    "moment": "^2.19.3",
    "node-sass": "^4.7.2",
    "nodemon": "^1.12.1",
    "postcss-loader": "^2.0.9",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-html-parser": "^2.0.2",
    "react-router-dom": "^4.2.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "superagent": "^3.8.1",
